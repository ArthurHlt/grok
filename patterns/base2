USERNAME [a-zA-Z0-9_-]+
USER %{USERNAME}




MONTHNUM (?:0?[1-9]|1[0-2])
MONTHDAY (?:3[01]|[1-2]?[0-9]|0?[1-9])




YEAR [0-9]+
HOUR (?:2[0123]|[01][0-9])
MINUTE (?:[0-5][0-9])
SECOND (?:(?:[0-5][0-9]|60)(?:[.,][0-9]+)?)
TIME (?!<[0-9])%{HOUR}:%{MINUTE}(?::%{SECOND})(?![0-9])
DATE_US %{MONTHNUM}[/-]%{MONTHDAY}[/-]%{YEAR}
DATE_EU %{YEAR:annee}[/-]%{MONTHNUM}[/-]%{MONTHDAY}
DATE %{DATE_US}|%{DATE_EU}
